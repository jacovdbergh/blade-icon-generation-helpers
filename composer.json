{
    "name": "codeat3/blade-icon-generation-helpers",
    "description": "codeat3's helper methods to use in blade-icon generation scripts",
    "license": "MIT",
    "require": {
        "illuminate/support": "^8.0|^9.0|^10.0",
        "codeat3/laravel-page-speed": "dev-master",
        "symfony/process": "^5.0|^6.0",
        "symfony/filesystem": "^5.0|^6.0",
        "codeat3/inlinestyle": "^2.0"
    },
    "repositories": [
        { "type": "vcs", "url": "https://github.com/jacovdbergh/laravel-page-speed" },
        {
            "type": "git",
            "url": "git@github.com:codeat3/inlinestyle.git"
        }
    ],
    "require-dev": {
        "codeat3/phpcs-styles": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Codeat3\\BladeIconGeneration\\": "src"
        }
    },
    "bin": [
        "bin/fetch-latest-icons"
    ],
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix"
    }
}
